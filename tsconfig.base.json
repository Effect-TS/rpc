{
  "include": [],
  "compilerOptions": {
    "strict": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "NodeNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "NodeNext",
    "incremental": true,
    "removeComments": false,
    "paths": {
      "@effect/rpc/test/*": [
        "./packages/rpc/test/*.js"
      ],
      "@effect/rpc/*": [
        "./packages/rpc/src/*.js"
      ],
      "@effect/rpc": [
        "./packages/rpc/src/index.js"
      ],
      "@effect/rpc-http/test/*": [
        "./packages/http/test/*.js"
      ],
      "@effect/rpc-http/*": [
        "./packages/http/src/*.js"
      ],
      "@effect/rpc-http": [
        "./packages/http/src/index.js"
      ],
      "@effect/rpc-http-node/test/*": [
        "./packages/http-node/test/*.js"
      ],
      "@effect/rpc-http-node/*": [
        "./packages/http-node/src/*.js"
      ],
      "@effect/rpc-http-node": [
        "./packages/http-node/src/index.js"
      ],
      "@effect/rpc-nextjs/test/*": [
        "./packages/nextjs/test/*.js"
      ],
      "@effect/rpc-nextjs/*": [
        "./packages/nextjs/src/*.js"
      ],
      "@effect/rpc-nextjs": [
        "./packages/nextjs/src/index.js"
      ],
      "@effect/rpc-workers/test/*": [
        "./packages/workers/test/*.js"
      ],
      "@effect/rpc-workers/*": [
        "./packages/workers/src/*.js"
      ],
      "@effect/rpc-workers": [
        "./packages/workers/index.js"
      ],
    },
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  }
}
