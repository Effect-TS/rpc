{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@effect/rpc/test/*": ["./packages/rpc/build/test/*"],
      "@effect/rpc/examples/*": ["./packages/rpc/build/examples/*"],
      "@effect/rpc/*": ["./packages/rpc/build/esm/*"],

      "@effect/rpc-http/test/*": ["./packages/http/build/test/*"],
      "@effect/rpc-http/examples/*": ["./packages/http/build/examples/*"],
      "@effect/rpc-http/*": ["./packages/http/build/esm/*"],
      "@effect/rpc-http": ["./packages/http/build/esm/index.js"],

      "@effect/rpc-http-node/test/*": ["./packages/http-node/build/test/*"],
      "@effect/rpc-http-node/examples/*": [
        "./packages/http-node/build/examples/*"
      ],
      "@effect/rpc-http-node/*": ["./packages/http-node/build/esm/*"],

      "@effect/rpc-nextjs/test/*": ["./packages/nextjs/build/test/*"],
      "@effect/rpc-nextjs/examples/*": ["./packages/nextjs/build/examples/*"],
      "@effect/rpc-nextjs/*": ["./packages/nextjs/build/esm/*"],

      "@effect/rpc-webworkers/test/*": ["./packages/webworkers/build/test/*"],
      "@effect/rpc-webworkers/examples/*": [
        "./packages/webworkers/build/examples/*"
      ],
      "@effect/rpc-webworkers/*": ["./packages/webworkers/build/esm/*"]
    }
  },
  "include": [
    "./packages/rpc/build/esm/**/*.js",
    "./packages/rpc/build/test/**/*.js",
    "./packages/rpc/build/examples/**/*.js",

    "./packages/http/build/esm/**/*.js",
    "./packages/http/build/test/**/*.js",
    "./packages/http/build/examples/**/*.js",

    "./packages/http-node/build/esm/**/*.js",
    "./packages/http-node/build/test/**/*.js",
    "./packages/http-node/build/examples/**/*.js"
  ]
}
